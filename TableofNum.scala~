class TableofList{

def calculate(series : List[Int])=
{
	for(iterator1 <- series)
	{	
                println(s"Table of $iterator1")
		for(iterator2 <- 1 to 10)
		{
			print(iterator1 * iterator2 + " ")
		}
              println("")
	}
}
}
object TableofNum extends App{
val list = List(1,2,3)
val obj=new TableofList
obj.calculate(list)
}
